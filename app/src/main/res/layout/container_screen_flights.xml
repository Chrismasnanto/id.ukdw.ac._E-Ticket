<?xml version="1.0" encoding="utf-8"?>

<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container_screen_flights"
    android:layout_width="match_parent"
    android:layout_height="844dp"
    android:background="@drawable/container_screen_flights">

    <ImageView
        android:id="@+id/container_bg_header"
        android:layout_width="390dp"
        android:layout_height="440dp"
        android:layout_gravity="top|left"
        android:scaleType="centerInside"
        android:src="@drawable/container_bg_header" />

    <FrameLayout
        android:id="@+id/container_bars_status_bars_iphone_dark"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginLeft="-1dp"
        android:layout_marginRight="1dp"
        android:layout_marginTop="3dp"
        android:layout_marginBottom="791dp">

        <FrameLayout
            android:id="@+id/container_frame"
            android:layout_width="350dp"
            android:layout_height="22dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="14dp"
            android:layout_gravity="top|left">

            <FrameLayout
                android:id="@+id/container_time_style"
                android:layout_width="54dp"
                android:layout_height="22dp"
                android:layout_gravity="top|left">

                <TextView
                    android:id="@+id/text_time"
                    android:layout_width="match_parent"
                    android:layout_height="22dp"
                    android:layout_gravity="center_vertical"
                    android:gravity="center_horizontal|center_vertical"
                    android:includeFontPadding="false"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:text="9:41"
                    android:textColor="#FFFFFFFF"
                    android:textSize="17dp" />
            </FrameLayout>

            <ImageView
                android:id="@+id/container_frame1"
                android:layout_width="80.15dp"
                android:layout_height="13dp"
                android:layout_marginLeft="269.85dp"
                android:layout_marginTop="4.1dp"
                android:layout_gravity="top|left"
                android:scaleType="centerInside"
                android:src="@drawable/container_frame1" />
        </FrameLayout>
    </FrameLayout>

    <FrameLayout
        android:id="@+id/container_frame11"
        android:layout_width="322dp"
        android:layout_height="34dp"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="112dp"
        android:layout_gravity="top|left">

        <LinearLayout
            android:id="@+id/container_frame3"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginRight="111dp"
            android:background="@drawable/container_frame3"
            android:clipToPadding="false"
            android:gravity="left|center_vertical"
            android:orientation="horizontal"
            android:paddingLeft="12dp"
            android:paddingTop="8dp"
            android:paddingBottom="-1dp">

            <ImageView
                android:id="@+id/container_arrow_right"
                android:layout_width="7.06dp"
                android:layout_height="9.24dp"
                android:layout_marginLeft="3.5dp"
                android:layout_marginTop="2.38dp"
                android:scaleType="centerInside"
                android:src="@drawable/container_arrow_right" />

            <TextView
                android:id="@+id/text_one_way"
                android:layout_width="wrap_content"
                android:layout_height="16dp"
                android:layout_marginLeft="6dp"
                android:gravity="left|center_vertical"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:singleLine="true"
                android:text="One-way"
                android:textColor="#FFFFFFFF"
                android:textSize="12dp" />
        </LinearLayout>

        <FrameLayout
            android:id="@+id/container_frame5"
            android:layout_width="100dp"
            android:layout_height="32dp"
            android:layout_marginRight="1dp"
            android:layout_gravity="center"
            android:background="@drawable/container_frame5"
            android:paddingBottom="9dp"
            android:paddingTop="9dp">

            <ImageView
                android:id="@+id/img_refresh"
                android:layout_width="14dp"
                android:layout_height="14dp"
                android:layout_gravity="top|left"
                android:scaleType="centerInside"
                android:src="@drawable/img_refresh" />

            <TextView
                android:id="@+id/text_round_trip"
                android:layout_width="wrap_content"
                android:layout_height="16dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="-1dp"
                android:layout_marginBottom="-2dp"
                android:layout_gravity="top|left"
                android:gravity="left|center_vertical"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:singleLine="true"
                android:text="Round-trip"
                android:textColor="#FF7060E5"
                android:textSize="12dp" />
        </FrameLayout>

        <FrameLayout
            android:id="@+id/container_frame7"
            android:layout_width="104dp"
            android:layout_height="34dp"
            android:layout_marginLeft="109dp"
            android:layout_gravity="center"
            android:background="@drawable/container_frame7"
            android:paddingBottom="10dp"
            android:paddingTop="10dp">

            <ImageView
                android:id="@+id/img_arrow"
                android:layout_width="14dp"
                android:layout_height="14dp"
                android:layout_gravity="top|left"
                android:scaleType="centerInside"
                android:src="@drawable/img_arrow" />

            <TextView
                android:id="@+id/text_multi_city"
                android:layout_width="wrap_content"
                android:layout_height="16dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="-1dp"
                android:layout_marginBottom="-2dp"
                android:layout_gravity="top|left"
                android:gravity="left|center_vertical"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:singleLine="true"
                android:text="Multi-city"
                android:textColor="#FFFFFFFF"
                android:textSize="12dp" />
        </FrameLayout>
    </FrameLayout>

    <FrameLayout
        android:id="@+id/container_frame9"
        android:layout_width="101dp"
        android:layout_height="40dp"
        android:layout_marginLeft="402dp"
        android:layout_marginTop="173.53dp"
        android:layout_gravity="top|left"
        android:background="@drawable/container_frame9"
        android:paddingBottom="8dp"
        android:paddingTop="8dp">

        <ImageView
            android:id="@+id/img_airplane"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_gravity="top|left"
            android:scaleType="centerInside"
            android:src="@drawable/img_airplane" />

        <TextView
            android:id="@+id/text_flight"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_marginLeft="34dp"
            android:layout_marginTop="2dp"
            android:layout_gravity="top|left"
            android:gravity="left|center_vertical"
            android:includeFontPadding="false"
            android:maxLines="1"
            android:singleLine="true"
            android:text="Flight"
            android:textColor="#FF121212"
            android:textSize="14dp" />
    </FrameLayout>

    <LinearLayout
        android:id="@+id/container_frame10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="170dp"
        android:layout_gravity="top|left"
        android:gravity="left|top"
        android:orientation="vertical">

        <FrameLayout
            android:layout_width="350dp"
            android:layout_height="388dp"
            android:background="@drawable/drawable_shape">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="18dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:layout_gravity="top|left"
                android:gravity="left|center_vertical"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:singleLine="true"
                android:text="From"
                android:textColor="#FF9B9B9B"
                android:textSize="12dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="21dp"
                android:layout_marginLeft="50dp"
                android:layout_marginTop="45dp"
                android:layout_gravity="top|left"
                android:gravity="left|center_vertical"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:singleLine="true"
                android:text="Yogyakarta (YIA)"
                android:textColor="#FF464646"
                android:textSize="14dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="21dp"
                android:layout_marginLeft="54dp"
                android:layout_marginTop="101dp"
                android:layout_gravity="top|left"
                android:gravity="left|center_vertical"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:singleLine="true"
                android:text="Denpasar (DPS)"
                android:textColor="#FF464646"
                android:textSize="14dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="18dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="76dp"
                android:layout_gravity="top|left"
                android:gravity="left|center_vertical"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:singleLine="true"
                android:text="To"
                android:textColor="#FF9B9B9B"
                android:textSize="12dp" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="16dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="44dp"
                android:layout_gravity="top|left"
                android:scaleType="centerInside"
                android:src="@drawable/drawable_image" />

            <ImageView